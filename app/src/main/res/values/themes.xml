<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.711proj" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#f77f00</item>
        <item name="colorPrimaryVariant">#023e8a</item>
        <item name="colorOnPrimary">#ffffff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#f77f00</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ButtonColor" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">#023e8a</item>
    </style>

    <style name="Theme.711proj.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.711proj.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.711proj.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>